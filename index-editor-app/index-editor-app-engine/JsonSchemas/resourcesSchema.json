{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "Resources": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "pageTitle": {
            "type": "string",
            "minLength": 1
          },
          "pageIcon": {
            "type": "string",
            "minLength": 1
          },
          "pageLink": {
            "type": "string",
            "format": "uri",
            "minLength": 1
          },
          "pageImage": {
            "type": "string",
            "format": "uri",
            "minLength": 1
          },
          "pageDescription": {
            "type": "string",
            "minLength": 1
          },
          "bulletpoints": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "minLength": 1
                },
                "link": {
                  "type": "string",
                  "format": "uri",
                  "minLength": 1
                },
                "description": {
                  "type": "string",
                  "minLength": 1
                },
                "linkPhrases": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "link": {
                        "type": "string",
                        "format": "uri",
                        "minLength": 1
                      },
                      "phrase": {
                        "type": "string",
                        "minLength": 1
                      }
                    },
                    "required": [ "link", "phrase" ]
                  }
                }
              },
              "required": [ "title", "link", "description", "linkPhrases" ]
            }
          }
        },
        "required": [ "pageTitle", "pageIcon", "pageLink", "pageImage", "pageDescription", "bulletpoints" ]
      }
    }
  },
  "required": [ "Resources" ]
}